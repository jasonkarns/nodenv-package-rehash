{
  "name": "nodenv-package-rehash",
  "version": "1.2.0",
  "private": true,
  "description": "Never run nodenv rehash again",
  "homepage": "https://github.com/nodenv/nodenv-package-rehash#readme",
  "license": "MIT",
  "author": "Jason Karns <jason@karns.name> (http://jason.karns.name)",
  "repository": {
    "type": "git",
    "url": "https://github.com/nodenv/nodenv-package-rehash.git"
  },
  "bugs": {
    "url": "https://github.com/nodenv/nodenv-package-rehash/issues"
  },
  "directories": {
    "bin": "bin",
    "lib": "libexec",
    "test": "test"
  },
  "scripts": {
    "test": "bats ${CI:+--tap} test",
    "publish:brew": "brew-publish $npm_package_name v$npm_package_version",
    "postversion": "git push origin master v$npm_package_version && npm run publish:brew"
  },
  "devDependencies": {
    "bats": "^0.4.2",
    "bats-assert": "^1.0.1",
    "bats-mock": "^1.0.1",
    "brew-publish": "git+https://github.com/jasonkarns/brew-publish.git#nodenv-tap"
  },
  "keywords": [
    "nodenv",
    "node",
    "package",
    "npm",
    "rehash",
    "global",
    "shim"
  ]
}
